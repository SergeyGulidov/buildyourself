<% provide(:title, t(:home) ) %>
<%= metamagic :description => "#{t(:meta_default_description)}", :keywords => "#{t(:meta_default_keywords)}" %>

<div class="row content content_bottom_margin">
  <div class="large-12 small-12 columns" >
   
    <%= render 'shared/flasher' %>
    <%= render 'shared/admin/admin_panel' %>

    <div class="row content">
      <div  class="large-9 small-12 columns">
        <% unless @type_vip.blank?  %>
          <%= render 'vip'%>
        <% end %>

        <div id="places">
          <%= render 'shared/gmap' %>
          <br>
          <br>
          <%= render @places %>
       </div>

        <div id="paginator">
          <%= paginate @places %>
        </div>
      </div>

      <div class="large-3 columns hide-for-small">

        <%= render 'shared/navigator' %>
        <%= render 'shared/login' %>
        <%= render 'shared/blog_panel'%>
        <%# render 'sponsor' %>
       </div>

    </div>
  </div>
</div> 


