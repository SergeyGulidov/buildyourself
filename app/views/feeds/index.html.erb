<div class="row content">
  <div class="small-8 small-centered columns">
    <%= render 'shared/flasher' %>
    <%= render 'shared/admin/admin_panel' %>
    <table>
      <thead>
        <tr>
          <th>Feed</th>
          <th></th>
        </tr>
      </thead>

      <% @feeds.each do |feed| %>
        <tr>
          <td><%= feed.message %></td>
          <td><%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>